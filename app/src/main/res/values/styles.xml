<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="join_edit_text">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColorHint">@color/white</item>
<!--        <item name="android:background">@drawable/bg_round_border</item>-->
        <item name="android:paddingTop">@dimen/dimSpace1</item>
        <item name="android:paddingBottom">@dimen/dimSpace1</item>
        <item name="android:paddingLeft">@dimen/dimen120px</item>
        <item name="android:paddingRight">@dimen/dimSpace1</item>
        <item name="android:layout_marginTop">@dimen/dimSpace1</item>
        <item name="android:textSize">@dimen/dimTextParagraph</item>
        <item name="android:textColor">@color/white</item>
<!--        <item name="android:fontFamily">@font/fontspring_demo_biennale_regular</item>-->
        <item name="android:focusableInTouchMode">true</item>
    </style>
    <style name="join_edit_text_label">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">Business Name</item>
<!--        <item name="android:fontFamily">@font/fontspring_demo_biennale_regular</item>-->
        <item name="android:layout_marginLeft">@dimen/dimSpace1</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/dimTextParagraph</item>
    </style>

    <!--  Common Styles  -->
    <style name="common_text_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
<!--        <item name="android:fontFamily">@font/helveticaneue</item>-->
        <item name="android:textSize">14.7sp</item>
    </style>
    <style name="common_linear_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
